{
  "manifest_version": 3,
  "name": "Echo360 Downloader",
  "version": "1.3",
  "key": "",
  "permissions": [
    "storage",
    "nativeMessaging",
    "webRequest",
    "cookies",
    "debugger"
  ],
  "host_permissions": [
    "*://content.echo360.org/*",
    "*://echo360.org/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Download Lecture",
    "default_popup": "popup.html"
  }
}
